{
  "name": "electron-quick-start",
  "productName": "Test App",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "prepare": "npm run rebuild",
    "build": "run-s clean build:*",
    "build:64": "run-s rebuild:64 package:64:*",
    "build:32": "run-s rebuild:32 package:32:*",
    "rebuild": "electron-rebuild",
    "rebuild:64": "npm run rebuild -- --arch x64",
    "rebuild:32": "npm run rebuild -- --arch ia32",
    "package": "electron-packager . --out dist --asar.unpack=*.node",
    "package:64:darwin": "npm run package -- --arch x64 --platform=darwin",
    "package:64:mas": "npm run package -- --arch x64 --platform=mas",
    "package:64:linux": "npm run package -- --arch x64 --platform=linux",
    "package:64:win32": "npm run package -- --arch x64 --platform=win32",
    "package:32:linux": "npm run package -- --arch ia32 --platform=linux",
    "package:32:win32": "npm run package -- --arch ia32 --platform=win32",
    "clean": "rimraf dist && mkdirp dist"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.4.1",
    "electron-packager": "^8.5.2",
    "electron-rebuild": "^1.5.7",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "standard": "^8.6.0"
  },
  "dependencies": {
    "cool-ascii-faces": "^1.3.4",
    "serialport": "^4.0.7"
  }
}
