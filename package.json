{
  "name": "electron-build-example",
  "productName": "Test App",
  "version": "1.0.1",
  "description": "A test app demonstrating an electron build system",
  "main": "main.js",
  "build": {
    "appId": "com.hypermodules.dat",
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 220,
          "y": 200
        },
        {
          "x": 448,
          "y": 200,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "nsis": {
      "perMachine": true
    }
  },
  "scripts": {
    "start": "electron .",
    "test": "standard",
    "postinstall": "install-app-deps",
    "rebuild": "electron-rebuild",
    "clean": "rimraf dist && mkdirp dist",
    "build": "build --dir",
    "dist": "build"
  },
  "repository": "https://github.com/bcomnes/electron-build-example",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.4.1",
    "electron-builder": "^14.4.0",
    "electron-rebuild": "^1.5.7",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "standard": "^8.6.0"
  },
  "dependencies": {
    "cool-ascii-faces": "^1.3.4",
    "serialport": "^4.0.7"
  }
}
